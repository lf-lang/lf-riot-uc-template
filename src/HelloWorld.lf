target uC

preamble {=
  #include "led.h"
=}

main reactor {
    timer t1(0, 1300msec)
    timer t2(0, 200msec)
    
    reaction(startup) {=
        LED0_ON;
        LED1_ON;
    =}

    reaction(t1) {=
        LED0_TOGGLE;
        printf("Toggle LED0\n");
    =}

    reaction(t2) {=
        LED1_TOGGLE;
        printf("Toggle LED1\n");
    =}
}
